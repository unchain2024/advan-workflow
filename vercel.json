{
  "buildCommand": "cd frontend-react && npm ci && npm run build",
  "outputDirectory": "frontend-react/dist",
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/output/(.*)",
      "destination": "/api/index.py"
    }
  ]
}
