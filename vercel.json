{
  "buildCommand": "cd frontend-react && npm install && npm run build",
  "outputDirectory": "frontend-react/dist",
  "installCommand": "pip install -r requirements.txt",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/output/(.*)",
      "destination": "/api/index.py"
    }
  ]
}
